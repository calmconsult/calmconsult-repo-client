<% (layout('/layout/boilerplate')) %>
<div class="container recently-added">
  <div class="row">
    <div class="col-lg-9">
      <h5>Collections</h5>
      <ul class="category-list">
        <% for(const collection of collections){%>
        <li><a href="/collections/<%=collection%> "><%=collection%></a></li>
        <%} %>
      </ul>

      <h5>Authors</h5>
      <ul class="category-list">
        <% for(const author of authors){%>
        <li><a href="/authors/<%=author%> "><%=author%></a></li>
        <%} %>
      </ul>

      <%-include('./partials/recentlyAdded')%>
    </div>
    <div class="col-lg-3"><%-include('./partials/sidemenu') %></div>
  </div>
</div>
